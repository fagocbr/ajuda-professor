{
  "title": "Ajuda do Professor",
  "language": "pt-BR",
  "author": "Material de ajuda para o app dos professores da Fagoc",
  "root": "./manuscript",
  "structure": {
    "readme": "readme.md",
    "glossary": "glossary.md",
    "summary": "summary.md"
  },
  "plugins": [
    "ga",
    "github",
    "prism",
    "-highlight",
    "github-edit"
  ],
  "pluginsConfig": {
    "disqus": {
      "shortName": "ajuda-professor"
    },
    "ga": {
      "token": ""
    },
    "github": {
      "url": "https://github.com/fagocbr/ajuda-professor"
    },
    "github-edit": {
        "repo": "fagocbr/ajuda-professor",
        "branch": "master/manuscript"
    }
  }
}
